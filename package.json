{
  "name": "mobx-youtube",
  "version": "1.0.0",
  "description": "An intro to mobx",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url https://martinbagshaw.github.io/mobx-youtube/",
    "test": "jest --watchAll --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinbagshaw/mobx-intro.git"
  },
  "author": "martinbagshaw",
  "license": "ISC",
  "homepage": "https://github.com/martinbagshaw/mobx-intro#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setuptests.ts"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "dayjs": "^1.9.5",
    "dotenv": "^8.2.0",
    "gh-pages": "^3.1.0",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "react-hot-loader": "^4.13.0",
    "typescript": "^4.0.5"
  }
}
